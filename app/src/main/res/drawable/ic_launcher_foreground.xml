<?xml version="1.0" encoding="utf-8"?>
<!-- AI Wave Key Logo (Concept A) - Foreground for Adaptive Icon -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="512"
    android:viewportHeight="512">
    
    <!-- AI Wave Monogram (centered and scaled for adaptive icon) -->
    <group
        android:translateX="256"
        android:translateY="256">
        
        <!-- Letter A (simplified) -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M -60,-50 L -40,50 L -20,50 L 0,-50 L -20,-50 Z"/>
        <path
            android:strokeColor="#FFFFFF"
            android:strokeWidth="8"
            android:fillColor="@android:color/transparent"
            android:pathData="M -40,0 L -20,0"/>
        
        <!-- Wave/Speech Pattern -->
        <path
            android:strokeColor="#FFFFFF"
            android:strokeWidth="10"
            android:fillColor="@android:color/transparent"
            android:pathData="M -30,0 Q -15,-15 0,0 Q 15,15 30,0"/>
        <path
            android:strokeColor="#FFFFFF"
            android:strokeWidth="10"
            android:fillColor="@android:color/transparent"
            android:pathData="M -30,20 Q -15,5 0,20 Q 15,35 30,20"/>
        
        <!-- Letter I -->
        <path
            android:strokeColor="#FFFFFF"
            android:strokeWidth="14"
            android:fillColor="@android:color/transparent"
            android:pathData="M 50,-50 L 50,50"/>
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M 50,-60 A 6,6 0 1,1 50,-48 A 6,6 0 1,1 50,-60 Z"/>
    </group>
</vector>

